{
    "name": "wearemode/craft-telescope",
    "description": "Take a peek into your Craft sites from a cloud dashboard",
    "type": "craft-plugin",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "craft-telescope"
    ],
    "support": {
        "docs": "https://github.com/wearemode/craft-telescope/",
        "issues": "https://github.com/wearemode/craft-telescope/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Simon Davies",
            "homepage": "https://simon-davies.name/"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1"
    },
    "autoload": {
        "psr-4": {
          "wearemode\\crafttelescope\\": "src/"
        }
    },
    "extra": {
        "name": "Craft Telescope",
        "handle": "craft-telescope",
        "hasCpSettings": true,
        "hasCpSection": false,
        "changelogUrl": "???",
        "class": "wearemode\\crafttelescope\\Crafttelescope"
    }
}
